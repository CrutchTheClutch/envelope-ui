{
  "name": "@clutchd/slot",
  "version": "2.0.12",
  "description": "A utility component that merges it's props onto it's child.",
  "keywords": [
    "clutchd",
    "slot",
    "ui"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clutchd/clutchd.git",
    "directory": "packages/slot"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --format esm --dts --minify",
    "dev": "tsup src/index.tsx --format esm --dts --watch",
    "prepublish": "bun run build",
    "bun-publish": "bun publish",
    "test": "bun test --coverage --coverage-reporter=lcov --preload='./preload.ts'"
  },
  "dependencies": {
    "@clutchd/compose-props": "workspace:*",
    "@clutchd/compose-refs": "workspace:*"
  },
  "devDependencies": {
    "@clutchd/bundlejs": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@radix-ui/react-slot": "1.1.0",
    "@testing-library/dom": "10.3.1",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@types/node": "22.1.0",
    "@types/react": "18.3.3",
    "react": "18.3.1",
    "tsup": "8.1.0"
  },
  "peerDependencies": {
    "react": "^18.0 || ^19.0"
  }
}
