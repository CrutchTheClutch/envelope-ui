{
  "name": "@clutchd/bundlejs",
  "version": "0.1.2",
  "description": "A small utility package to determine the size of a js bundle.",
  "keywords": [
    "clutchd",
    "bundlejs"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "packages/bundlejs"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "pnpm run build",
    "test": "jest"
  },
  "dependencies": {
    "@hazae41/foras": "2.0.8",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@clutchd/jest": "1.0.2",
    "@clutchd/tsconfig": "1.2.0",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.2",
    "@types/node-fetch": "2.6.4",
    "jest": "29.5.0",
    "tsup": "7.1.0",
    "typescript": "5.1.3"
  }
}
