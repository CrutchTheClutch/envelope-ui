{
  "name": "@clutchd/bundlejs",
  "version": "0.2.0",
  "description": "A small utility package to determine the size of a js bundle.",
  "keywords": [
    "clutchd",
    "bundlejs"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "packages/bundlejs"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "main": "dist/index",
  "module": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "pnpm run build",
    "test": "jest"
  },
  "dependencies": {
    "@hazae41/foras": "2.0.8",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@clutchd/jest": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@types/jest": "29.5.2",
    "@types/node": "20.9.2",
    "@types/node-fetch": "2.6.4",
    "jest": "29.6.1",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  }
}
