    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.0.1",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "react": "18.3.1",
    "tsup": "8.3.5"
  },
  "peerDependencies": {
    "react": "^18.0 || ^19.0"
