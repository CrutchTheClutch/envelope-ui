    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@clutchd/config": "*",
    "@clutchd/eslint-config-clutchd": "1.1.0",
    "@prisma/client": "4.9.0",
    "jest": "29.3.1",
    "next-auth": "4.18.6",
    "prisma": "4.14.0",
    "ts-jest": "29.0.3",
    "tsup": "6.7.0",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "@prisma/client": "4.9.0",
    "next-auth": "4.18.6"
  }
}
