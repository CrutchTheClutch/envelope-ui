{
  "name": "@clutchd/next-auth-prisma-adapter",
  "version": "1.0.1",
  "keywords": [
    "clutchd",
    "next-auth",
    "prisma",
    "next"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "packages/next-auth-prisma-adapter"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "prisma generate && tsup src/index.ts --format esm,cjs --dts --external react",
