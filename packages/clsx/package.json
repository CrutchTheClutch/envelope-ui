{
  "name": "@clutchd/clsx",
  "version": "1.0.0",
  "main": "dist/index.js",
  "description": "A tinier (440B), more performant implementation of clsx.",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "clean": "rm -rf .next .turbo node_modules coverage dist package-lock.json",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@clutchd/config": "*",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "node-fetch": "^2.6.1",
    "terser": "^5.16.1",
    "tsup": "^6.5.0",
    "typescript": "^4.8.4"
  }
}
