{
  "name": "@clutchd/clsx",
  "version": "0.1.3",
  "main": "dist/index.js",
  "description": "A tinier (228B), more performant implementation of clsx.",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "benchmark": "npm run build && node src/benchmark",
    "build": "tsup src/index.js --format esm,cjs --dts --external react",
    "clean": "rm -rf .next .turbo node_modules coverage dist package-lock.json",
    "dev": "tsup src/index.js --format esm,cjs --watch --dts --external react",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@clutchd/config": "*",
    "benchmark": "^2.1.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "terser": "^5.16.1",
    "tsup": "^6.5.0"
  },
  "dependencies": {
    "clsx": "^1.2.1"
  }
}
