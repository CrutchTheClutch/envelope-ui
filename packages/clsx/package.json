    "prepublish": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@clutchd/config": "*",
    "@clutchd/eslint-config-clutchd": "^1.0.0",
    "@clutchd/tsconfig": "^1.0.0",
    "jest": "^29.3.1",
    "node-fetch": "^2.6.1",
    "terser": "^5.16.1",
    "tsup": "^6.5.0",
    "typescript": "^4.8.4"
  }
