{
  "name": "@clutchd/clsx",
  "version": "1.0.1",
  "description": "A tinier (430B), more performant implementation of clsx.",
  "keywords": [
    "clutchd",
    "clsx"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "packages/clsx"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
