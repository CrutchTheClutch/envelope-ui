{
  "name": "@clutchd/clsx",
  "version": "1.0.7",
  "description": "A tinier (225B), more performant implementation of clsx.",
  "keywords": [
    "clutchd",
    "clsx"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "packages/clsx"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "source": "./src/index.ts",
  "main": "dist/index",
  "module": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "benchmark": "node ./benchmark/clsx.benchmark.js",
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "pnpm run build",
    "test": "jest",
    "validate": "pnpm run build && pnpm run benchmark && pnpm run test"
  },
  "devDependencies": {
    "@clutchd/bundlejs": "workspace:*",
    "@clutchd/jest": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@types/jest": "29.5.2",
    "@types/node": "20.9.2",
    "clsx": "2.0.0",
    "jest": "29.6.1",
    "node-fetch": "2.6.7",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  }
}
