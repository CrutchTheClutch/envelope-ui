  },
  "devDependencies": {
    "@clutchd/jest": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@types/jest": "29.5.2",
    "@types/node": "20.4.1",
    "jest": "29.6.1",
    "node-fetch": "2.6.1",
    "tsup": "7.1.0",
    "typescript": "5.1.6"
  }
