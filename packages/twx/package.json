    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@clutchd/clsx": "^1.0.2"
  },
  "devDependencies": {
    "@clutchd/config": "*",
    "@clutchd/eslint-config-clutchd": "^1.0.0",
    "@clutchd/tsconfig": "^1.0.1",
    "jest": "^29.3.1",
    "node-fetch": "^2.6.1",
    "terser": "^5.16.1",
    "tsup": "^6.6.0",
    "typescript": "^4.8.4"
  }
}
