{
  "name": "@clutchd/twx",
  "version": "1.0.1",
  "description": "Combining the powers of @clutchd/clsx and tailwind-merge.",
  "keywords": [
    "clutchd",
    "clsx",
    "twx",
    "tailwind"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "packages/clsx"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "clean": "rm -rf .next .turbo node_modules coverage dist package-lock.json",
