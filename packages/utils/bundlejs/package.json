{
  "name": "@clutchd/bundlejs",
  "version": "0.1.2",
  "description": "A small utility package to determine the size of a js bundle.",
  "keywords": [
    "clutchd",
    "bundlejs"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "packages/utils/bundlejs"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "clean": "rm -rf .next .turbo node_modules coverage dist package-lock.json",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@clutchd/eslint-config-clutchd": "1.1.2",
    "@clutchd/jest": "1.0.0",
    "@clutchd/tsconfig": "1.1.0",
    "@types/node-fetch": "^2.6.4",
    "tsup": "6.7.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@hazae41/foras": "^2.0.8",
    "node-fetch": "2.6.1"
  }
}
