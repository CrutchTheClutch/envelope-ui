    "prepublish": "pnpm run build",
    "test": "jest"
  },
  "dependencies": {
    "@hazae41/foras": "2.0.8",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@clutchd/jest": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@types/jest": "29.5.12",
    "@types/node-fetch": "2.6.11",
    "jest": "29.7.0",
    "tsup": "8.1.0",
    "typescript": "5.5.3"
  }
}
