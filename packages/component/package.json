{
  "name": "@clutchd/component",
  "version": "1.1.0",
  "keywords": [
    "clutchd",
    "component",
    "tailwind",
    "next"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "packages/component"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "clean": "rm -rf .next .turbo node_modules coverage dist package-lock.json",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@clutchd/compose-props": "^1.1.3",
    "@clutchd/compose-refs": "^1.0.2"
  },
  "devDependencies": {
    "@clutchd/config": "*",
    "@clutchd/eslint-config-clutchd": "^1.0.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.2.0",
    "@types/react": "^18.0.35",
    "@types/testing-library__jest-dom": "^5.14.5",
    "jest": "^29.3.1",
    "react": "^18.2.0",
    "ts-jest": "^29.0.3",
    "tsup": "^6.5.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
