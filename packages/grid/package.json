    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "next": "^12.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "dependencies": {
    "@clutchd/component": "^0.7.0",
    "@clutchd/clsx": "0.1.1",
    "@clutchd/tailwind": "0.5.0"
  }
