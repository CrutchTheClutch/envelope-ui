{
  "name": "@clutchd/get-initials",
  "version": "1.2.6",
  "keywords": [
    "clutchd",
    "get-initials",
    "initials"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
