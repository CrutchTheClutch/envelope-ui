    "@clutchd/compose-props": "workspace:*"
  },
  "devDependencies": {
    "@clutchd/bundlejs": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@testing-library/dom": "10.3.1",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@types/node": "22.1.0",
    "@types/react": "18.3.3",
    "next": "14.2.5",
    "react": "18.3.1",
    "tsup": "8.1.0"
  },
  "peerDependencies": {
    "next": "^14.0",
    "react": "^18.0 || ^19.0"
  }
