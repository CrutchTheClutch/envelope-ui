{
  "name": "@clutchd/navigator",
  "version": "2.0.8",
  "description": "A @clutchd/link powered component with additional features for navigation.",
  "keywords": [
    "clutchd",
    "navigator",
    "tailwind",
    "next"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clutchd/clutchd.git",
    "directory": "packages/link"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --format esm --dts --minify",
    "dev": "tsup src/index.tsx --format esm --dts --watch",
    "prepublish": "bun run build",
    "publish": "bun publish",
    "test": "bun test --coverage --coverage-reporter=lcov --preload='./preload.ts'"
  },
  "dependencies": {
    "@clutchd/component": "workspace:*",
    "@clutchd/compose-props": "workspace:*",
    "@clutchd/link": "workspace:*"
  },
  "devDependencies": {
    "@clutchd/bundlejs": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@testing-library/dom": "10.3.1",
    "@testing-library/react": "16.0.0",
    "@types/node": "22.1.0",
    "@types/react": "18.3.3",
    "next": "14.2.5",
    "react": "18.3.1",
    "tsup": "8.1.0"
  },
  "peerDependencies": {
    "next": "^14.0",
    "react": "^18.0 || ^19.0"
  }
}
