{
  "name": "@clutchd/flex",
  "version": "0.1.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "clean": "rm -rf .next .turbo node_modules coverage package-lock.json",
    "test": "jest --passWithNoTests",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@clutchd/config": "*",
    "postcss": "^8.4.16",
    "react": "^18.1.0",
    "ts-jest": "^28.0.5",
    "tsup": "^5.12.6",
    "typescript": "^4.5.5",
    "next": "^12.1.0"
  },
  "peerDependencies": {
    "next": "^12.1.0",
    "next-auth": "^4.10.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "dependencies": {
    "@clutchd/clsx": "0.1.1",
    "@clutchd/component": "^0.6.1",
    "@clutchd/tailwind": "0.4.1"
  }
}
