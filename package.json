{
  "name": "clutchd",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "benchmark": "turbo run benchmark --concurrency=1",
    "build": "turbo run build",
    "changeset": "changeset",
    "clean": "turbo run clean && rm -rf .next .turbo node_modules coverage package-lock.json",
    "dev": "turbo run dev --no-cache --parallel",
    "format": "prettier --write \"**/*.{js,json,jsx,ts,tsx,md,mdx}\"",
    "postinstall": "manypkg check",
    "lint": "turbo run lint",
    "release": "turbo run build && changeset publish",
    "test": "turbo run test",
    "typesync": "npx typesync",
    "version-packages": "changeset version"
  },
  "dependencies": {
    "@changesets/cli": "^2.22.0",
    "@clutchd/eslint-config-clutchd": "*",
    "@manypkg/cli": "^0.19.2",
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
