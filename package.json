{
  "name": "clutchd",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "benchmark": "turbo run benchmark",
    "build": "turbo run build",
    "changeset": "changeset",
    "dev": "turbo run dev --parallel",
    "format": "biome format . --write",
    "lint": "biome check . --write",
    "preinstall": "bun x only-allow bun",
    "publish": "turbo run publish --continue && changeset tag",
    "test": "turbo run test"
  },
  "dependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "2.26.2",
    "@types/bun": "1.1.11",
    "turbo": "2.2.3",
    "typescript": "5.6.3"
  },
  "engines": {
    "bun": ">=1.1.0",
    "node": ">=20.0.0"
  },
  "packageManager": "bun@1.1.34",
  "patchedDependencies": {
    "@changesets/cli@2.26.2": "patches/@changesets%2Fcli@2.26.2.patch"
  }
}
