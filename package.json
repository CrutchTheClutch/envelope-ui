{
  "name": "@clutchd/ui",
  "version": "0.0.0-development",
  "description": "A React component library designed around consistent, and simple web design.",
  "author": "clutchd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CrutchTheClutch/ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "build-storybook -c .storybook -o build",
    "start": "start-storybook",
    "dev": "start-storybook -p 6006 || $PORT",
    "prepare": "npm run format && npm run build",
    "commit": "npx git-cz",
    "format": "prettier-eslint --write \"src/**/*.{ts,tsx,js,jsx,mdx}\""
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "tailwindcss": "^2.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-backgrounds": "^6.0.26",
    "@storybook/addon-docs": "^6.0.26",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.0.26",
    "@storybook/theming": "^6.2.9",
    "cz-conventional-changelog": "^3.1.0",
    "prettier-eslint-cli": "^5.0.0",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tailwindcss": "^2.1.4"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
