{
  "name": "@clutchd/ui",
  "version": "0.0.0-development",
  "description": "A React component library designed around consistent, and simple web design.",
  "author": "clutchd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "build-storybook -c .storybook -o build && rollup -c",
    "start": "start-storybook",
    "dev": "start-storybook -p 6006 || $PORT",
    "prepare": "npm run format && npm run build",
    "commit": "npx git-cz",
    "format": "prettier-eslint --write \"$PWD/src/**/*.js\" \"$PWD/src/**/*.mdx\""
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "prismjs": "^1.17.x",
    "styled-components": "^4.x"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@rollup/plugin-url": "^4.0.2",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-essentials": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "@svgr/rollup": "^5.3.1",
    "@typescript-eslint/parser": "^2.29.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.6.0",
    "node-sass": "^4.14.0",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dnd": "^7.7.0",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^3.1.1",
    "semantic-release": "^17.0.7",
    "storybook-addon-deps": "^2.0.0",
    "storybook-chromatic": "^3.5.3",
    "styled-components": "^4.4.1",
    "vue": "^2.6.11"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
