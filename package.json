{
  "name": "clutchd",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "benchmark": "turbo run benchmark",
    "build": "turbo run build",
    "changeset": "changeset",
    "clean": "find . -type d \\( -name '.next'  -o -name '.turbo' -o -name 'coverage' -o -name 'dist' -o -name 'node_modules' \\) -prune -print -exec rm -rf '{}' \\;",
    "dev": "turbo run dev --parallel",
    "format": "biome format --write .",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "manypkg check",
    "lint": "turbo run lint",
    "release": "turbo run build && changeset publish",
    "test": "pnpm format && turbo run test",
    "typesync": "npx typesync",
    "version-packages": "changeset version"
  },
  "dependencies": {
    "@biomejs/biome": "1.9.0",
    "@changesets/cli": "2.26.2",
    "@manypkg/cli": "0.21.2",
    "turbo": "^2.0.11"
  },
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "manypkg": {
    "ignoredRules": ["INTERNAL_MISMATCH"]
  }
}
