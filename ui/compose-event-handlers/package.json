{
  "name": "@clutchd/compose-event-handlers",
  "version": "3.0.0",
  "description": "A small package that provides multiple event-handler utility functions.",
  "keywords": [
    "clutchd",
    "compose-event-handlers",
    "compose-preventable-event-handlers",
    "event-handlers",
    "merge-event-handlers"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "ui/compose-event-handlers"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "dist/index",
  "module": "dist/index",
  "source": "./src/index.ts",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "pnpm run build",
    "test": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@clutchd/bundlejs": "workspace:*",
    "@clutchd/jest": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@types/jest": "29.5.2",
    "@types/node": "20.9.2",
    "jest": "29.6.1",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  }
}
