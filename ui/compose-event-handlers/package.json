{
  "name": "@clutchd/compose-event-handlers",
  "version": "2.0.2",
  "description": "A small utility intended to quickly compose multiple event handlers into a single handler.",
  "keywords": [
    "clutchd",
    "compose-event-handlers",
    "event-handlers"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "ui/compose-event-handlers"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "source": "./src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "pnpm run build",
    "test": "jest"
  },
  "dependencies": {
    "@clutchd/merge-event-handlers": "1.0.2"
  },
  "devDependencies": {
    "@clutchd/bundlejs": "0.1.3",
    "@clutchd/jest": "1.0.2",
    "@clutchd/tsconfig": "1.2.1",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.2",
    "jest": "29.5.0",
    "tsup": "7.1.0",
    "typescript": "5.1.3"
  }
}
