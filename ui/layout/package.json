  "devDependencies": {
    "@clutchd/jest": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@types/jest": "29.5.2",
    "@types/node": "20.4.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.6",
    "jest": "29.6.1",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "tsup": "7.2.0",
    "typescript": "5.1.6"
