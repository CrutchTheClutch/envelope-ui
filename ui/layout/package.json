{
  "name": "@clutchd/layout",
  "version": "2.1.2",
  "keywords": [
    "clutchd",
    "layout",
    "tailwind",
    "next"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "packages/layout"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "main": "dist/index",
  "module": "dist/index",
