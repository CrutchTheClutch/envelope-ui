{
  "name": "@clutchd/slot",
  "version": "1.0.14",
  "description": "A utility component that merges it's props onto it's child.",
  "keywords": [
    "clutchd",
    "slot",
    "ui"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "ui/slot"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "source": "./src/index.tsx",
  "main": "dist/index",
  "module": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --minify",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "pnpm run build",
    "test": "jest"
  },
  "dependencies": {
    "@clutchd/compose-props": "workspace:*",
    "@clutchd/compose-refs": "workspace:*"
  },
  "devDependencies": {
    "@clutchd/bundlejs": "workspace:*",
    "@clutchd/jest": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.2",
    "@types/node": "20.4.1",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.6",
    "jest": "29.6.1",
    "react": "18.2.0",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": "^18.2"
  }
}
