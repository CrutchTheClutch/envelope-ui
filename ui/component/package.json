{
  "name": "@clutchd/component",
  "version": "3.0.0",
  "description": "A primitive component intended to be used as a foundation for all other ui components.",
  "keywords": [
    "clutchd",
    "component",
    "ui"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "ui/component"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "source": "./src/index.tsx",
  "main": "dist/index",
  "module": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --minify",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "bun run build",
    "test": "bun test --coverage"
  },
  "dependencies": {
    "@clutchd/slot": "workspace:*"
  },
  "devDependencies": {
    "@clutchd/bundlejs": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@clutchd/twx": "workspace:*",
    "@types/react": "18.2.47",
    "react": "18.2.0",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "@clutchd/twx": "^2.0.0",
    "react": "^18.2"
  },
  "peerDependenciesMeta": {
    "@clutchd/twx": {
      "optional": true
    }
  }
}
