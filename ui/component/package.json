{
  "name": "@clutchd/component",
  "version": "2.0.6",
  "description": "A primitive component intended to be used as a foundation for all other ui components.",
  "keywords": [
    "clutchd",
    "component",
    "ui"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "ui/component"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "source": "./src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --minify",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "pnpm run build",
    "test": "jest"
  },
  "dependencies": {
    "@clutchd/slot": "1.0.6"
  },
  "devDependencies": {
    "@clutchd/bundlejs": "0.1.3",
    "@clutchd/jest": "1.0.2",
    "@clutchd/tsconfig": "1.2.1",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "jest": "29.5.0",
    "react": "18.2.0",
    "tsup": "7.1.0",
    "typescript": "5.1.3"
  },
  "peerDependencies": {
    "react": "^18.2"
  }
}
