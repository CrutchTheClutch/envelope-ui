{
  "name": "@clutchd/compose-refs",
  "version": "1.1.6",
  "description": "A small utility intended to compose multiple refs into a single ref.",
  "keywords": [
    "clutchd",
    "compose-refs",
    "refs"
  ],
  "bugs": {
    "url": "https://github.com/clutchd/clutchd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clutchd/clutchd",
    "directory": "ui/compose-refs"
  },
  "author": "Clutchd, LLC",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "source": "./src/index.tsx",
  "main": "dist/index",
  "module": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --minify",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "prepublish": "bun run build",
    "test": "bun test --coverage"
  },
  "devDependencies": {
    "@clutchd/bundlejs": "workspace:*",
    "@clutchd/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.2.47",
    "react": "18.2.0",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": "^18.2"
  }
}
